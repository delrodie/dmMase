<form method="post" action="{{ path('backend_faq_delete', {'id': faq.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cet element?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ faq.id) }}">
    <button class="btn btn-danger mt-5">Supprimer</button>
</form>
