<form method="post" action="{{ path('backend_entreprise_delete', {'id': entreprise.id}) }}" onsubmit="return confirm('Voulez-vous supprimer vraiment cette entreprise?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entreprise.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
