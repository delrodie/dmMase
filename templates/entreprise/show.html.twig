{% extends 'base.html.twig' %}

{% block title %}Entreprise{% endblock %}

{% block body %}
    <h1>Entreprise</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entreprise.id }}</td>
            </tr>
            <tr>
                <th>RaisonSociale</th>
                <td>{{ entreprise.raisonSociale }}</td>
            </tr>
            <tr>
                <th>Siege</th>
                <td>{{ entreprise.siege }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entreprise.adresse }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ entreprise.telephone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entreprise.email }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ entreprise.annee }}</td>
            </tr>
            <tr>
                <th>Domaine</th>
                <td>{{ entreprise.domaine }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entreprise.type }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ entreprise.slug }}</td>
            </tr>
            <tr>
                <th>Valid</th>
                <td>{{ entreprise.valid ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ entreprise.createdAt ? entreprise.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('entreprise_index') }}">back to list</a>

    <a href="{{ path('entreprise_edit', {'id': entreprise.id}) }}">edit</a>

    {{ include('entreprise/_delete_form.html.twig') }}
{% endblock %}
